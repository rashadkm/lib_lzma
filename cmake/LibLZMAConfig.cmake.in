# Compute paths
get_filename_component(LibLZMA_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LibLZMA_INCLUDE_DIR "@CONF_INCLUDE_DIRS@" PARENT_SCOPE)

set(BUILD_SHARED_LIBS @BUILD_SHARED_LIBS@)

if(NOT TARGET @LIB_NAME@ AND NOT LibLZMA_BINARY_DIR)
  include("${LibLZMA_CMAKE_DIR}/LibLZMATargets.cmake")
endif()

if(MSVC)
  if(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_IMPLIB_@CONFIG_TYPE@)
  else(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
  endif(BUILD_SHARED_LIBS)
else()
  get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
endif()

set(LibLZMA_LIBRARY ${LIB_LIB_FILENAME} PARENT_SCOPE)
#compatiblity
set(LibLZMA_LIBRARIES ${LIB_LIB_FILENAME} PARENT_SCOPE)

set(LibLZMA_FOUND TRUE)
